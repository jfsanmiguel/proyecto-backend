
<article>
    
<header>
    <h1>Shop ticket</h1>
</header>
<p><strong>First Name: </strong>{{user.first_Name}}</p>
<p><strong>Last Name: </strong>{{user.last_Name}}</p>
<p><strong>E-mail: </strong>{{user.email}}</p>
<p><strong>Ticket Code: </strong>{{ticket.code}}</p>
<p><strong>Amount: </strong>{{ticket.amount}}</p>

    </form>
<table>
    <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Price</th>
        <th>Thumbnail</th>
        <th>Code</th>
        <th>Stock</th>
        <th>Category</th>
    </tr>
    <tr>
        {{#each products}}
            <tr>
                <td>{{product.title}}</td>
                <td>{{product.description}}</td>
                <td>{{product.price}}</td>
                <td>{{product.thumbnail}}</td>
                <td>{{product.code}}</td>
                <td>{{product.stock}}</td>
                <td>{{product.category}}</td>
            </tr>
        {{/each}}
    </tr>
</table>
<footer>
<a href="/api/session/logout"role="button">Log-out</a>
<a href="/current"role="button">Go to cart</a>

</footer>
</article>