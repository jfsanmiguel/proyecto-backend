<h1>Cart from: {{customer}}</h1>
<article>
<table>
    <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Price</th>
        <th>Thumbnail</th>
        <th>Code</th>
        <th>Stock</th>
        <th>Category</th>
        <th>Quantity</th>
    </tr>
    <tr>
        
        {{#each products}}
            <tr>
                <td>{{product.title}}</td>
                <td>{{product.description}}</td>
                <td>{{product.price}}</td>
                <td>{{product.thumbnail}}</td>
                <td>{{product.code}}</td>
                <td>{{product.stock}}</td>
                <td>{{product.category}}</td>
                <td>{{quantity}}</td>
            </tr>
        {{/each}}
        
    </tr>
</table>
</article>