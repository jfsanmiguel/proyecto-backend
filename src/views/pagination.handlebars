<h1>Products</h1>
<article>
    <table>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Price</th>
            <th>Thumbnail</th>
            <th>Code</th>
            <th>Stock</th>
            <th>Category</th>
        </tr>
        <tbody>
            {{#each payload}}
            <tr>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>{{this.thumbnail}}</td>
                <td>{{this.code}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <p>actual page: {{page}}</p>
    

    
    <a href="{{prevLink}}" role="button" {{#unless hasPrevPage}}disabled {{/unless}}>Previous page</a>
    <a href="{{nextLink}}" role="button" {{#unless hasNextPage}}disabled {{/unless}}>Nextpage</a>
</article>